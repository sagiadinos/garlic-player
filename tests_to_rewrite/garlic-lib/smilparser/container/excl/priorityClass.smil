<?xml version="1.0" encoding="utf-8"?>
<body>
	<excl xml:id="with multiple priorityClass tags">
		<priorityClass xml:id="prioA1">
			<img xml:id="imgA0010"/>
		</priorityClass>
		<priorityClass xml:id="prioA2">
			<img xml:id="imgA0009"/>
		</priorityClass>
		<priorityClass xml:id="prioA3">
			<seq xml:id="seq02" repeatCount="2">
				<img xml:id="imgA0006"/>
				<img xml:id="imgA0007"/>
				<img xml:id="imgA0008"/>
			</seq>
		</priorityClass>
		<priorityClass xml:id="prioA4" higher="pause">
			<seq xml:id="seq01" repeatCount="indefinite">
				<img xml:id="imgA0001" />
				<img xml:id="imgA0002" />
				<img xml:id="imgA0003" />
				<img xml:id="imgA0004" />
				<img xml:id="imgA0005" />
			</seq>		
		</priorityClass>	
	</excl>	
	<excl xml:id="without priorityClass tag">
		<img xml:id="imgB0001" />
		<img xml:id="imgB0002" />
		<img xml:id="imgB0003" />
		<img xml:id="imgB0004" />
		<img xml:id="imgBA0005" />
	</excl>	
	<excl xml:id="not allowed mixed priorityClass tag and direct excl childs">
		<priorityClass xml:id="prioC1">
			<img xml:id="imgC0010"/>
		</priorityClass>
		<img xml:id="imgCA0001" />
		<img xml:id="imgC0002" />
		<img xml:id="imgC0003" />
		<img xml:id="imgC0004" />
		<img xml:id="imgCA0005" />
		<priorityClass>
			<img xml:id="imgCA0006"/>
		</priorityClass>
	</excl>	
</body>