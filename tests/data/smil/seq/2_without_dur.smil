<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<smil>
	<head>
		<meta name="title" content="Testing media in seq which have no duration" />
		<layout>
			<root-layout id="display:0" width="1920" height="1080" />
			<region regionName="screen" top="0" left="0" width="1920" height="1080" z-index="1" backgroundColor="#000" />
		</layout>
	</head>
	<body>
	
	<!--
		
		sequence should be begin,black,10,black,10,end
	
	-->

		<img xml:id="img_begin" src="../../images/begin.jpg" dur="2s" fit="meetbest" />
		<seq xml:id="seq2" repeatCount="2">
			<img xml:id="img0006" src="../../images/0006.jpg" fit="meetbest" />
			<img xml:id="img0007" src="../../images/0007.jpg" fit="meetbest" />
			<img xml:id="img0008" src="../../images/0008.jpg" fit="meetbest" />
			<img xml:id="img0009" src="../../images/0009.jpg" fit="meetbest" />
			<img xml:id="img0010" src="../../images/0010.jpg" dur="1s" fit="meetbest" />
		</seq>	
		<img xml:id="img_end" src="../../images/end.jpg" dur="2s" fit="meetbest" />
	</body>
</smil>
